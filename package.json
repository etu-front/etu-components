{
  "name": "@etu/components",
  "version": "3.0.0",
  "license": "ISC",
  "description": "components of etu",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:etu-front/etu-components.git"
  },
  "scripts": {
    "dev": "npm run build -- -w",
    "build": "npm run clean && tsc --build tsconfig.json",
    "clean": "rm -rf ./dist && rm -rf ./es",
    "build:commonjs": "tsc -m commonjs --outDir ./dist --skipLibCheck true",
    "build:es": "tsc -m es6 --outDir ./es  --skipLibCheck true",
    "pub": "npm run build && node ./scripts/publish.js",
    "test": "jest --collect-coverage --verbose",
    "test-all": "npm run test -- -u"
  },
  "author": "handy",
  "dependencies": {
    "classnames": "^2.3.1",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "nprogress": "^0.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@cfaester/enzyme-adapter-react-18": "^0.6.0",
    "@types/enzyme": "^3.10.3",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.11",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^29.7.0",
    "jest-runner": "^29.7.0",
    "jest-styled-components": "^7.0.0",
    "prettier": "^1.17.1",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "react-test-renderer": "^18.0.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^29.2.4",
    "typescript": "^4.0.0",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}